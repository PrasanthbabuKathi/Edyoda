   "source": [
    "#Write a Python program to square the elements of a list using map() function.\n",
    "\n",
    "\n",
    "#Sample List: [4, 5, 2, 9]\n",
    "\n",
    "#Square the elements of the list:\n",
    "\n",
    "#[16, 25, 4, 81]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "354b5033",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[16, 25, 4, 81]\n"
     ]
    }
   ],
   "source": [
    "list1 = [4, 5, 2, 9]\n",
    "\n",
    "result = map(lambda x : x**2, list1)\n",
    "print(list(result))"
   ]
  }
